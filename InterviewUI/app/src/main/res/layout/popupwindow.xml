<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:minWidth="@dimen/px2dp_200"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/px2dp_10"
    android:background="@drawable/button_bg_white1"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_totalMoney"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_14"
        android:gravity="center_horizontal"
        android:includeFontPadding="false"
        android:text="@string/default_unit_money"
        android:textColor="#333333"
        android:textSize="@dimen/px2sp_24" />

    <LinearLayout
        android:id="@+id/ll_invest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_14"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#39bced" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="投资产品"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_investproduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_insure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_12"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#f5c64b" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="保险产品"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_insureproduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_oversea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_12"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#ffa24e" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="海外产品"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_seaproduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_fund"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_12"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#aecc6d" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="公募基金"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_fund"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_allowance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_12"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#ef5285" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="管理津贴"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_allowance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_team"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_12"
        android:gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px2dp_10"
        android:layout_marginRight="@dimen/px2dp_10"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/px2dp_6"
            android:layout_height="match_parent"
            android:background="#ab53f2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="米多奖励"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />

        <TextView
            android:id="@+id/tv_teamreward"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_10"
            android:includeFontPadding="false"
            android:text="@string/default_unit_money"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_20" />
    </LinearLayout>


</LinearLayout>
